    <declare-styleable name="CardVerificationCodeEditText">
        <attr name="fieldName"/>

        <attr name="inputType"/>

        <attr name="text"/>
        <attr name="enabled"/>
        <attr name="textSize"/>
        <attr name="textColor" />
        <attr name="maxLines"/>
